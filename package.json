{
  "name": "docketeer",
  "productName": "Docketeer",
  "version": "18.0.0",
  "license": "MIT",
  "description": "A modern GUI for Docker that provides developers with both management and visualization tools.",
  "author": "Docketeer, Docker Dragon 18",
  "scripts": {
    "test": "jest --verbose",
    "jest-preview": "jest-preview",
    "test:debug": "npm-run-all -p test jest-preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@jest/globals": "^29.3.1",
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-preview": "^0.3.1",
    "npm-run-all": "latest",
    "react": "^18.2.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-source-labs/Docketeer.git"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "ts-jest"
    },
    "preset": "ts-jest/presets/js-with-ts",
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!d3|d3-array|internmap|delaunator|robust-predicates)"
    ]
  },
  "bugs": {
    "url": "https://github.com/open-source-labs/Docketeer/issues"
  },
  "homepage": "https://github.com/open-source-labs/Docketeer#readme",
  "keywords": []
}
